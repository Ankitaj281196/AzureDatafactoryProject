{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "GHTS12"
		},
		"DataLake_conn_LK_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLake_conn_LK'"
		},
		"SQL_DB_Genric_LK_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQL_DB_Genric_LK_SQL'"
		},
		"Sql_LS_ON prem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Sql_LS_ON prem'",
			"defaultValue": "Integrated Security=True;Data Source=TEST1;Initial Catalog=master"
		},
		"Sql_LS_ON prem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sql_LS_ON prem'"
		},
		"DataLake_conn_LK_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adl2232.dfs.core.windows.net/"
		},
		"Sql_LS_ON prem_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Admin1111"
		},
		"Every 2min_properties_pipeline2_parameters_schemOne": {
			"type": "string",
			"defaultValue": "saleslt"
		},
		"Every 2min_properties_pipeline2_parameters_Tableone": {
			"type": "string",
			"defaultValue": "Customer"
		},
		"Storage_Event_Trigger_properties_pipeline2_parameters_schemOne": {
			"type": "string",
			"defaultValue": "saleslt"
		},
		"Storage_Event_Trigger_properties_pipeline2_parameters_Tableone": {
			"type": "string",
			"defaultValue": "Address"
		},
		"Storage_Event_Trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/fbb84bc6-ecd6-4933-8ccc-a505ac5a0cc0/resourceGroups/Practice/providers/Microsoft.Storage/storageAccounts/adl2232"
		},
		"Tumbling Trigger_properties_pipeline_parameters_schemOne": {
			"type": "string",
			"defaultValue": "saleslt"
		},
		"Tumbling Trigger_properties_pipeline_parameters_Tableone": {
			"type": "string",
			"defaultValue": "Customer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "GHTS12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLake_conn_LK_accountKey": {
						"value": "[parameters('DataLake_conn_LK_accountKey')]"
					},
					"SQL_DB_Genric_LK_SQL_connectionString": {
						"value": "[parameters('SQL_DB_Genric_LK_SQL_connectionString')]"
					},
					"Sql_LS_ON prem_connectionString": {
						"value": "[parameters('Sql_LS_ON prem_connectionString')]"
					},
					"Sql_LS_ON prem_password": {
						"value": "[parameters('Sql_LS_ON prem_password')]"
					},
					"DataLake_conn_LK_properties_typeProperties_url": {
						"value": "[parameters('DataLake_conn_LK_properties_typeProperties_url')]"
					},
					"Sql_LS_ON prem_properties_typeProperties_userName": {
						"value": "[parameters('Sql_LS_ON prem_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "GHTS12_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Every 2min_properties_pipeline2_parameters_schemOne": {
						"value": "[parameters('Every 2min_properties_pipeline2_parameters_schemOne')]"
					},
					"Every 2min_properties_pipeline2_parameters_Tableone": {
						"value": "[parameters('Every 2min_properties_pipeline2_parameters_Tableone')]"
					},
					"Storage_Event_Trigger_properties_pipeline2_parameters_schemOne": {
						"value": "[parameters('Storage_Event_Trigger_properties_pipeline2_parameters_schemOne')]"
					},
					"Storage_Event_Trigger_properties_pipeline2_parameters_Tableone": {
						"value": "[parameters('Storage_Event_Trigger_properties_pipeline2_parameters_Tableone')]"
					},
					"Storage_Event_Trigger_properties_typeProperties_scope": {
						"value": "[parameters('Storage_Event_Trigger_properties_typeProperties_scope')]"
					},
					"Tumbling Trigger_properties_pipeline_parameters_schemOne": {
						"value": "[parameters('Tumbling Trigger_properties_pipeline_parameters_schemOne')]"
					},
					"Tumbling Trigger_properties_pipeline_parameters_Tableone": {
						"value": "[parameters('Tumbling Trigger_properties_pipeline_parameters_Tableone')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'GHTS12_ArmTemplate_0')]"
			]
		}
	]
}