{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "GHTS12"
		},
		"Every 2min_properties_pipeline2_parameters_schemOne": {
			"type": "string",
			"defaultValue": "saleslt"
		},
		"Every 2min_properties_pipeline2_parameters_Tableone": {
			"type": "string",
			"defaultValue": "Customer"
		},
		"Storage_Event_Trigger_properties_pipeline2_parameters_schemOne": {
			"type": "string",
			"defaultValue": "saleslt"
		},
		"Storage_Event_Trigger_properties_pipeline2_parameters_Tableone": {
			"type": "string",
			"defaultValue": "Address"
		},
		"Storage_Event_Trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/fbb84bc6-ecd6-4933-8ccc-a505ac5a0cc0/resourceGroups/Practice/providers/Microsoft.Storage/storageAccounts/adl2232"
		},
		"Tumbling Trigger_properties_pipeline_parameters_schemOne": {
			"type": "string",
			"defaultValue": "saleslt"
		},
		"Tumbling Trigger_properties_pipeline_parameters_Tableone": {
			"type": "string",
			"defaultValue": "Customer"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Dataflow_example')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Data flow1",
						"type": "ExecuteDataFlow",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataflow": {
								"referenceName": "dataflow_for hectranet",
								"type": "DataFlowReference",
								"parameters": {},
								"datasetParameters": {
									"inputfile": {},
									"sink1": {}
								}
							},
							"staging": {},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": [],
				"lastPublishTime": "2023-08-31T08:59:11Z"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Every 2min')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "pipeline2",
							"type": "PipelineReference"
						},
						"parameters": {
							"schemOne": "[parameters('Every 2min_properties_pipeline2_parameters_schemOne')]",
							"Tableone": "[parameters('Every 2min_properties_pipeline2_parameters_Tableone')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 1,
						"startTime": "2023-08-27T17:19:00",
						"timeZone": "India Standard Time"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Storage_Event_Trigger')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "pipeline2",
							"type": "PipelineReference"
						},
						"parameters": {
							"schemOne": "[parameters('Storage_Event_Trigger_properties_pipeline2_parameters_schemOne')]",
							"Tableone": "[parameters('Storage_Event_Trigger_properties_pipeline2_parameters_Tableone')]"
						}
					}
				],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/landing/blobs/vvvv",
					"blobPathEndsWith": ".txt",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('Storage_Event_Trigger_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"
					]
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Tumbling Trigger')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipeline": {
					"pipelineReference": {
						"referenceName": "pipeline2",
						"type": "PipelineReference"
					},
					"parameters": {
						"schemOne": "[parameters('Tumbling Trigger_properties_pipeline_parameters_schemOne')]",
						"Tableone": "[parameters('Tumbling Trigger_properties_pipeline_parameters_Tableone')]"
					}
				},
				"type": "TumblingWindowTrigger",
				"typeProperties": {
					"frequency": "Minute",
					"interval": 5,
					"startTime": "2023-08-28T05:06:00Z",
					"delay": "00:00:00",
					"maxConcurrency": 50,
					"retryPolicy": {
						"count": 2,
						"intervalInSeconds": 30
					},
					"dependsOn": []
				}
			},
			"dependsOn": []
		}
	]
}